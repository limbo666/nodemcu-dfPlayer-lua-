		/*正则表达式 替换括号,尖括号等*/  
		function toTxt(str) {  
			var RexStr = /\<|\>|\"|\'|\&/g;
			str = str.replace(RexStr, function(MatchStr) {  
				switch (MatchStr) {  
				case "<":  
					//return "&lt;";
					return "";   
					break;  
				case ">":  
					//return "&gt;";  
					return "";
					break;  
				case "\"":  
					//return "&quot;"; 
					return ""; 
					break;  
				case "'":  
					//return "&#39;";  
					return "";
					break;  
				case "&":  
					//return "&amp;"; 
					return ""; 
					break;  
				default:  
					break;  
				};
			});
			return str;  
		}  
	
