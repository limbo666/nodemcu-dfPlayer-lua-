		function isInAnniversary(){
			var d = new Date();
			var dateBegin = new Date('8 8 2016,0:0:0');
			var dateEnd = new Date('9 9 2016,23:59:59');
			var timeDifference = d.getTimezoneOffset()*60000;
			var timeBegin = dateBegin.getTime()+timeDifference+480*60000;
			var timeEnd = dateEnd.getTime()+timeDifference+480*60000;
			var timeNow = d.getTime();
			return (timeNow >= timeBegin && timeNow <= timeEnd);
		}
		
