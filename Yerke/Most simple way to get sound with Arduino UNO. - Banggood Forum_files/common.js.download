	function txtHighLight(text, keywords){
		if(typeof(text) == 'undefined' || text.length == 0){
			return false;
		}
		if(typeof(keywords) == 'undefined' || keywords.length == 0){
			return false;
		}
		var arr = keywords.split(' ');
		for(var i = 0; i < arr.length; i++){
			var word = arr[i];
			var reg = '/'+word+'/';
			var rep = '<b>'+word+'</b>';
			eval("text = text.replace("+reg+", '"+rep+"');");
		}
		return text;
	}
	
